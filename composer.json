{
    "name": "brokeyourbike/country-casts-laravel",
    "description": "Cast Laravel country code attributes from ISO3 to ISO2",
    "type": "library",
    "license": "MPL-2.0",
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\CountryCasts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\CountryCasts\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8|^9",
        "league/iso3166": "^4.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9.5",
        "illuminate/database": "^8|^9"
    },
    "version": "0.1.2",
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
